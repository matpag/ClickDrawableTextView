<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CsDrawableViewManager">
        <!-- start drawable properties -->
        <attr name="csStartDrawable" format="reference" />
        <attr name="csStartDrawableHeight" format="dimension" />
        <attr name="csStartDrawableWidth" format="dimension" />
        <attr name="csStartDrawableVisible" format="boolean" />
        <attr name="csStartDrawableTint" format="reference|color" />
        <!-- Blending mode used to apply the drawable tint. -->
        <attr name="csStartDrawableTintMode" format="enum">
            <!-- The tint is drawn on top of the drawable.
                 [Sa + (1 - Sa)*Da, Rc = Sc + (1 - Sa)*Dc] -->
            <enum name="src_over" value="3" />
            <!-- The tint is masked by the alpha channel of the drawable. The drawable’s
                 color channels are thrown out. [Sa * Da, Sc * Da] -->
            <enum name="src_in" value="5" />
            <!-- The tint is drawn above the drawable, but with the drawable’s alpha
                 channel masking the result. [Da, Sc * Da + (1 - Sa) * Dc] -->
            <enum name="src_atop" value="9" />
            <!-- Multiplies the color and alpha channels of the drawable with those of
                 the tint. [Sa * Da, Sc * Dc] -->
            <enum name="multiply" value="14" />
            <!-- [Sa + Da - Sa * Da, Sc + Dc - Sc * Dc] -->
            <enum name="screen" value="15" />
            <!-- Combines the tint and drawable color and alpha channels, clamping the
                 result to valid color values. Saturate(S + D) -->
            <enum name="add" value="16" />
        </attr>

        <!-- end drawable properties -->
        <attr name="csEndDrawable" format="reference" />
        <attr name="csEndDrawableHeight" format="dimension" />
        <attr name="csEndDrawableWidth" format="dimension" />
        <attr name="csEndDrawableVisible" format="boolean" />
        <attr name="csEndDrawableTint" format="reference|color" />
        <!-- Blending mode used to apply the drawable tint. -->
        <attr name="csEndDrawableTintMode" format="enum">
            <!-- The tint is drawn on top of the drawable.
                 [Sa + (1 - Sa)*Da, Rc = Sc + (1 - Sa)*Dc] -->
            <enum name="src_over" value="3" />
            <!-- The tint is masked by the alpha channel of the drawable. The drawable’s
                 color channels are thrown out. [Sa * Da, Sc * Da] -->
            <enum name="src_in" value="5" />
            <!-- The tint is drawn above the drawable, but with the drawable’s alpha
                 channel masking the result. [Da, Sc * Da + (1 - Sa) * Dc] -->
            <enum name="src_atop" value="9" />
            <!-- Multiplies the color and alpha channels of the drawable with those of
                 the tint. [Sa * Da, Sc * Dc] -->
            <enum name="multiply" value="14" />
            <!-- [Sa + Da - Sa * Da, Sc + Dc - Sc * Dc] -->
            <enum name="screen" value="15" />
            <!-- Combines the tint and drawable color and alpha channels, clamping the
                 result to valid color values. Saturate(S + D) -->
            <enum name="add" value="16" />
        </attr>

        <!-- top drawable properties -->
        <attr name="csTopDrawable" format="reference" />
        <attr name="csTopDrawableHeight" format="dimension" />
        <attr name="csTopDrawableWidth" format="dimension" />
        <attr name="csTopDrawableVisible" format="boolean" />
        <attr name="csTopDrawableTint" format="reference|color" />
        <!-- Blending mode used to apply the drawable tint. -->
        <attr name="csTopDrawableTintMode" format="enum">
            <!-- The tint is drawn on top of the drawable.
                 [Sa + (1 - Sa)*Da, Rc = Sc + (1 - Sa)*Dc] -->
            <enum name="src_over" value="3" />
            <!-- The tint is masked by the alpha channel of the drawable. The drawable’s
                 color channels are thrown out. [Sa * Da, Sc * Da] -->
            <enum name="src_in" value="5" />
            <!-- The tint is drawn above the drawable, but with the drawable’s alpha
                 channel masking the result. [Da, Sc * Da + (1 - Sa) * Dc] -->
            <enum name="src_atop" value="9" />
            <!-- Multiplies the color and alpha channels of the drawable with those of
                 the tint. [Sa * Da, Sc * Dc] -->
            <enum name="multiply" value="14" />
            <!-- [Sa + Da - Sa * Da, Sc + Dc - Sc * Dc] -->
            <enum name="screen" value="15" />
            <!-- Combines the tint and drawable color and alpha channels, clamping the
                 result to valid color values. Saturate(S + D) -->
            <enum name="add" value="16" />
        </attr>

        <!-- bottom drawable properties -->
        <attr name="csBottomDrawable" format="reference" />
        <attr name="csBottomDrawableHeight" format="dimension" />
        <attr name="csBottomDrawableWidth" format="dimension" />
        <attr name="csBottomDrawableVisible" format="boolean" />
        <attr name="csBottomDrawableTint" format="reference|color" />
        <!-- Blending mode used to apply the drawable tint. -->
        <attr name="csBottomDrawableTintMode" format="enum">
            <!-- The tint is drawn on top of the drawable.
                 [Sa + (1 - Sa)*Da, Rc = Sc + (1 - Sa)*Dc] -->
            <enum name="src_over" value="3" />
            <!-- The tint is masked by the alpha channel of the drawable. The drawable’s
                 color channels are thrown out. [Sa * Da, Sc * Da] -->
            <enum name="src_in" value="5" />
            <!-- The tint is drawn above the drawable, but with the drawable’s alpha
                 channel masking the result. [Da, Sc * Da + (1 - Sa) * Dc] -->
            <enum name="src_atop" value="9" />
            <!-- Multiplies the color and alpha channels of the drawable with those of
                 the tint. [Sa * Da, Sc * Dc] -->
            <enum name="multiply" value="14" />
            <!-- [Sa + Da - Sa * Da, Sc + Dc - Sc * Dc] -->
            <enum name="screen" value="15" />
            <!-- Combines the tint and drawable color and alpha channels, clamping the
                 result to valid color values. Saturate(S + D) -->
            <enum name="add" value="16" />
        </attr>
    </declare-styleable>

</resources>